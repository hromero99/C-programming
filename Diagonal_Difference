#include <assert.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int n,i,s1=0,s2=0,s3=0,j;
        scanf("%d",&n);
    int a[n][n];
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            if((a[i][j]>=-100)&&(a[i][j]<=100))
            {
            if(i==j)
            {
                s1=s1+a[i][j];
            }
            if((i+j)==n-1)
            {
                s2=s2+a[i][j];
            }
        }
        }
    }
    s3=s1-s2;
    if(s3<0)
        s3=s3*-1;
    printf("%d",s3);
    return 0;
}
